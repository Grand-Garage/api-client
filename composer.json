{
    "name": "grandgarage/api-client",
    "description": "a simple rest api client for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Murauer",
            "email": "hallo@christophmurauer.com"
        }
    ],
    "keywords": [
        "laravel",
        "rest",
        "api"
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^7|^8|^9"
    },
    "require-dev": {
        "orchestra/testbench": "^6|^7"
    },
    "autoload": {
        "psr-4": {
            "Grandgarage\\ApiClient\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Grandgarage\\ApiClient\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
